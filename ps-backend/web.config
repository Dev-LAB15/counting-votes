<configuration>
  <appSettings>
    <!--<add key="pythonpath" value="%SystemDrive%\home\site\wwwroot" />
    <add key="WSGI_HANDLER" value="hostingstart-python.application" />-->
    
  </appSettings>
  <system.webServer>        
      <handlers>
        <add name="iisnode" path="app/server.js" verb="*" modules="iisnode"/>
      </handlers>
      <rewrite>
        <rules>
            <rule name="DynamicContent">
                 <match url="/*" />
                 <action type="Rewrite" url="app/server.js"/>
            </rule>
       </rules>
      </rewrite>
    </system.webServer>
</configuration>